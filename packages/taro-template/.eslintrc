{
  "extends": ["taro/react"],
  "parserOptions": {
    "requireConfigFile": false
  },
  "rules": {
    "react/jsx-uses-react": "off",
    "react/react-in-jsx-scope": "off",
    "import/first": "off",
    "import/no-commonjs": "off",
    "@typescript-eslint/no-unused-vars": "off",
    "import/newline-after-import": "off"
  }
}
